{
  "name": "react-arborist",
  "version": "2.1.0",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/module.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/brimdata/react-arborist",
  "homepage": "https://react-arborist.netlify.app",
  "keywords": [
    "react",
    "arborist",
    "react-arborist",
    "treeview",
    "tree",
    "vitualized",
    "dnd",
    "multiselection",
    "filterable"
  ],
  "dependencies": {
    "react-dnd": "^14.0.3",
    "react-dnd-html5-backend": "^14.0.1",
    "react-window": "^1.8.6",
    "redux": "^4.1.1",
    "use-sync-external-store": "^1.2.0"
  },
  "scripts": {
    "build": "run-p 'build:**'",
    "build:js": "parcel build --target main --target module",
    "build:types": "tsc --outDir dist",
    "watch:js": "parcel watch --target main --target module --no-hmr --no-cache",
    "watch:types": "tsc --outDir dist --watch",
    "watch": "run-p 'watch:**'",
    "prepack": "yarn build"
  },
  "peerDependencies": {
    "react": ">= 16.14",
    "react-dom": ">= 16.14"
  },
  "devDependencies": {
    "@parcel/core": "^2.3.2",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.0",
    "@types/react-window": "^1.8.5",
    "@types/use-sync-external-store": "^0.0.3",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.3.2",
    "typescript": "^4.6.2"
  }
}
