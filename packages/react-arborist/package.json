{
  "name": "react-arborist",
  "version": "3.3.0-rc.1",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "types": "main/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build:cjs": "tsc --outDir dist/main",
    "build:es": "tsc --outDir dist/module --module es2022 --moduleResolution node",
    "build": "npm-run-all clean -p 'build:**'",
    "clean": "rimraf dist",
    "prepack": "yarn build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brimdata/react-arborist.git"
  },
  "homepage": "https://react-arborist.netlify.app",
  "bugs": "https://github.com/brimdata/react-arborist/issues",
  "keywords": [
    "react",
    "arborist",
    "react-arborist",
    "treeview",
    "tree",
    "vitualized",
    "dnd",
    "multiselection",
    "filterable"
  ],
  "dependencies": {
    "react-dnd": "^14.0.3",
    "react-dnd-html5-backend": "^14.0.1",
    "react-window": "^1.8.6",
    "redux": "^4.1.1",
    "use-sync-external-store": "^1.2.0"
  },
  "peerDependencies": {
    "react": ">= 16.14",
    "react-dom": ">= 16.14"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.0",
    "@types/react-window": "^1.8.5",
    "@types/use-sync-external-store": "^0.0.3",
    "jest": "^29.4.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "typescript": "^5.3.3"
  }
}
